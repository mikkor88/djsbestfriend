<tr>
		<td class="artist">
		  <%= record.artist %>
		</td>
		<td class="title">
			 <%= link_to record.title, record %>
		</td>
		<td class="year">
			<%= record.year.strftime("%Y") unless record.year.nil? %>
		</td>
		<td class="type">
			<%= record.record_type %>
		</td>
		<td>
		<%= render 'records/own_form' %>
		</td>
</tr>